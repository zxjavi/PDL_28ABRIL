function imprime (string s, string msg, int f)	/* función que recibe 3 argumentos */
{
	print (s); print (msg); print (f);
	print ("\n");	// imprime un salto de línea */
	/* finaliza la ejecución de la función (en este caso, se podría omitir) */
}


